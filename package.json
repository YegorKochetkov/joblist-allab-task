{
	"name": "joblist-allab-task",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"deploy": "sh deploy.sh",
		"prepare": "husky install",
		"add-pre-commit": "npx husky add .husky/pre-commit \"yarn lint-staged\"",
		"pre-commit": "lint-staged",
		"format": "npx prettier --write . --ignore-path .gitignore",
		"lint:write": "npx eslint ./src/**/* --fix",
		"fix-style": "yarn format && yarn lint:write"
	},
	"lint-staged": {
		"src/**/*.{js,mjs,jsx,cjs,ts,tsx}": [
			"yarn fix-style"
		]
	},
	"dependencies": {
		"@commitlint/cli": "^17.2.0",
		"@commitlint/config-conventional": "^17.2.0",
		"@googlemaps/react-wrapper": "^1.1.35",
		"@types/google.maps": "^3.50.5",
		"framer-motion": "^7.6.12",
		"husky": "^8.0.2",
		"lint-staged": "^13.0.3",
		"react": "^18.2.0",
		"react-alice-carousel": "^2.6.6",
		"react-dom": "^18.2.0",
		"react-icons": "^4.6.0",
		"react-paginate": "^8.1.4",
		"react-responsive": "^9.0.0",
		"react-router-dom": "^6.4.3",
		"react-share": "^4.4.1",
		"styled-components": "^5.3.6",
		"twin.macro": "^3.0.0-rc.5",
		"use-deep-compare-effect": "^1.8.1",
		"zustand": "^4.1.4"
	},
	"devDependencies": {
		"@types/node": "^18.11.9",
		"@types/react": "^18.0.22",
		"@types/react-dom": "^18.0.7",
		"@types/styled-components": "^5.1.26",
		"@typescript-eslint/eslint-plugin": "^5.42.1",
		"@typescript-eslint/parser": "^5.42.1",
		"@vitejs/plugin-react": "^2.2.0",
		"autoprefixer": "^10.4.13",
		"babel-plugin-macros": "^3.1.0",
		"babel-plugin-styled-components": "^2.0.7",
		"eslint": "^8.27.0",
		"eslint-plugin-react": "^7.31.10",
		"postcss": "^8.4.18",
		"tailwindcss": "^3.2.1",
		"typescript": "^4.8.4",
		"vite": "^3.2.0"
	},
	"babelMacros": {
		"twin": {
			"preset": "styled-components"
		}
	}
}
